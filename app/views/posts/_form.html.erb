<%= form_with model: @post, local: true do |form| %>
  <p>
    <%= form.label :spot_name %>
  </p>
  <p>
    <%= form.text_field :spot_name, required: true %>
  </p>
  <p>
    <%= form.label :content %>
  </p>
    <%= form.text_area :content, required: true %>
  <p>
    <%= form.submit "投稿" %>
  </p>
<% end %>